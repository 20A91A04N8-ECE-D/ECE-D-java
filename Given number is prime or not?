import java.util.*;
class prime
{
	public static void main(String args[])
	{
		Scanner sc=new Scanner(System.in);
		int n,c=0,i;
		System.out.println("enter a number");
		n=sc.nextInt();
		for(i=1;i<=n;i++)
		{
		  if(n%i==0)
		  c++;
		} 
		  if(c==2)
		 System.out.println(n+"is a prime");
		 else
		 System.out.println(n+"is not a prime");
		
	}
}
